// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model readings {
  id                Int      @id @default(autoincrement())
  mmol_l            Float
  trend_arrow       String
  trend_description String
  datetime          DateTime
}
